{
  "name": "viral-content-finder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./node_modules/next/dist/bin/next dev",
    "build": "node ./node_modules/next/dist/bin/next build",
    "start": "node ./node_modules/next/dist/bin/next start",
    "lint": "node ./node_modules/eslint/bin/eslint.js",
    "format": "node ./node_modules/prettier/bin/prettier.cjs --write .",
    "format:check": "node ./node_modules/prettier/bin/prettier.cjs --check .",
    "typecheck": "node ./node_modules/typescript/bin/tsc --noEmit",
    "test": "node ./node_modules/.bin/vitest",
    "test:ui": "node ./node_modules/.bin/vitest --ui",
    "test:run": "node ./node_modules/.bin/vitest run",
    "worker": "node --import tsx/esm src/workers/index.ts",
    "postinstall": "node ./node_modules/.bin/prisma generate",
    "prepare": "husky"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@hookform/resolvers": "^5.2.2",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.5",
    "bullmq": "^5.70.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "franc-min": "^6.2.0",
    "ioredis": "^5.9.3",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "node-cron": "^4.2.1",
    "nuqs": "^2.8.8",
    "papaparse": "^5.5.3",
    "prisma": "^7.4.1",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.2",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "swr": "^2.4.0",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "node ./node_modules/eslint/bin/eslint.js --fix",
      "node ./node_modules/prettier/bin/prettier.cjs --write"
    ],
    "*.{js,jsx,mjs,cjs}": [
      "node ./node_modules/prettier/bin/prettier.cjs --write"
    ],
    "*.{json,css,md}": [
      "node ./node_modules/prettier/bin/prettier.cjs --write"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
